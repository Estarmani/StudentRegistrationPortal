@model IEnumerable<Profile>
@{
    Layout = null;
    var items = Model ?? new List<Profile>();
    var k = 0;
}
<table class="table table-striped table-bordered display text-nowrap dataTable">
    <thead>
        <tr>
            <th>S/N</th>
            <th>Surname</th>
            <th>First Name</th>
            <th>Other Names</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in items)
        {
            k++;
            <tr>
                <td>@k</td>
                <td>@item.Surname</td>
                <td>@item.FirstName</td>
                <td>@item.OtherNames</td>
                <td>@item.Gender.ToString()</td>
                <td>@item.DateOfBirth</td>
                <td>@item.Email</td>
                <td>
                    <a href="@Url.Action("_EditProfile", "Profiles", new {id = item.Id})" data-modal="" title="Edit Profiler"><i class="bi bi-pencil-square"></i></a>
                    <a href="javascript:deleteItem('@item.Id')"><i class="bi bi-trash-fill"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>